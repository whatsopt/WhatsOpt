<%= render 'toolbar', ope: @ope %>

<% if @ope.sensitivity_analysis? %>
<div id="sensitivity_plotter" data-mda="<%= @mda.to_whatsopt_ui_json %>" data-ope="<%= @ope.to_plotter_json %>" data-api-key="<%=current_user.api_key%>"></div>
<%= append_javascript_pack_tag 'sensitivity_plotter_application' %>
<% else %>
<div id="plotter" data-mda="<%= @mda.to_whatsopt_ui_json %>" data-uq-mode="<%= @mda.uq_mode? %>" data-ope="<%= @ope.to_plotter_json %>" data-api-key="<%=current_user.api_key%>"></div>
<%= append_javascript_pack_tag 'plotter_application' %>
<% end %>